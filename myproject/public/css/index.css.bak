	  *{
			margin:0px;
			padding:0px;
			border:none;
		}
		ul li{list-style: none;}
		body{min-width:1200px;text-align: center;}
		.banner{background-image:url(images/indeximages/1.jpg);
						width:100%;height:600px;
						background-repeat: no-repeat;
						background-size:cover;
						background-position:center;}
		.header{ margin:0 auto;
		     width:1200px;height:100px;}
		.header_nav{
			margin-left:300px;
			padding-top:45px;
		}
		
		.header_nav ul li{
			list-style-type:none;
			display:inline-block;
			margin-left:50px;
			
		}
		.header_nav ul li a{
		   
			text-decoration:none;
			font-size:20px;
			color:#fff;
			margin-top:10px;
			float:left;
			
		}
		.header_nav img{
			display:inline-block;
			float:left;
		}
		.header_nav_domn .img22{
			
			position:absolute;
			left:50%;
			right:50%;
			bottom:0px;
		}
		.header_nav_domn .img23{
			position:absolute;
			left:50%;
			right:50%;
			padding-left:80px;
			bottom:60px;
		}
		.search_txt input{
			 width:700px;
			 height:50px;
			
			 border-radius:5px;
			 padding-left:15px;
			 font-size:16px;
		}
		.search_txt{
		width:1200px;
		text-align:center;	
		}
		.search,.header_nav_domn{
			width:1200px;
			height:230px;
			margin:0 auto;
			position:relative;
		}
		@keyframes change{
			0%{ 
				transform: translateY( 0px);
			}
			50%{
				transform: translateY( 10px);
			}
		
		}
		.header_nav_domn{animation: change 3s linear 1s;
			animation-iteration-count: infinite;
			
			}
		.hot_nav{
			width:1200px;
			height:44px;
